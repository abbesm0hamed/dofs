output "eDP-1" {
    mode "1920x1200@60.001" // laptop
    scale 1.2 // laptop

    // mode "1920x1080@100.000" // desktop
    // scale 1 // desktop

    transform "normal"
    position x=1920 y=0  // Move laptop to the right
    variable-refresh-rate
}
output "HDMI-A-1" {
    mode "1920x1080@144"
    scale 1.0
    transform "normal"
    position x=0 y=0
    variable-refresh-rate
}
